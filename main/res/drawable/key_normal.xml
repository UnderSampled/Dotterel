<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape >
            android:shape="rectangle" >

            <corners android:radius="8dp"/>
            <solid android:color="@color/KEY_OUTLINE" />

        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="0dp"
        android:right="1dp"
        android:top="1dp">
       <shape android:shape="rectangle" >
            <solid android:color="@color/KEY_COLOR" />
            <corners android:radius="8dp"/>
        </shape>
    </item>
</layer-list>
