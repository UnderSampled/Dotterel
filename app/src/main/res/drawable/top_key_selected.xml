<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
<item>
    <shape android:shape="rectangle" >
        <corners android:radius="5dp"/>
        <gradient
            android:startColor="#77AA00"
            android:centerColor="#000"
            android:endColor="#000"
            android:centerY="0.8"
            android:angle="90"
            android:type="linear" />
    </shape>
</item>
<item
    android:bottom="1dp"
    android:left="1dp"
    android:right="1dp"
    android:top="1dp">
    <shape android:shape="rectangle" >
        <solid android:color="#202E00" />
        <corners android:radius="5dp"/>
    </shape>
</item>
</layer-list>
